{
    "name": "slick/cache",
    "description": "Cache component for Slick Framework",
    "license": "MIT",
    "homepage": "https://github.com/slickframework/cache",
    "keywords": ["slick", "cache", "memcached"],
    "authors": [
        {
            "name": "Filipe Silva",
            "email": "silvam.filipe@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
