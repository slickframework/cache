{
  "name": "slick/cache",
  "description": "An implementation of PSR-6/PSR-16 cache library.",
  "license": "MIT",
  "homepage": "https://github.com/slickframework/cache",
  "keywords": ["slick", "cache", "memcached", "PSR-6", "PSR-16"],
  "version": "v1.2.1",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Slick\\Cache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slick\\Cache\\Functional\\": "functional"
    }
  },
  "require": {
    "php": ">=7.0",
    "psr/cache": "^1.0@dev"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.4",
    "phpunit/phpunit": "^6.3",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "scripts": {
    "test": "phpspec run",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2-dev",
      "dev-develop": "2.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
